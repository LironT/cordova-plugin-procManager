<?xml version="1.0" encoding="UTF-8"?>
<plugin id="org.apache.cordova.procManager" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0">
  <name>Exec</name>
  <description>Cordova Plugin to manage processes</description>
  <license>Apache 2.0</license>
  <keywords>cordova,process,command</keywords>
  <js-module name="procManager" src="www/procManager.js">
    <clobbers target="procManager"/>
  </js-module>
  <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
      <feature name="procManager">
        <param name="android-package" value="org.apache.cordova.plugin.procManager"/>
        <param name="onload" value="true" />
      </feature>
    </config-file>
    <source-file src="src/org/apache/cordova/plugin/procManager.java" target-dir="src/org/apache/cordova/procManager" />
  </platform>
</plugin>
